<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <TextView
        android:id="@+id/exception_class_name_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Exception" />

    <TextView
        android:id="@+id/launch_coroutine_job_hash_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="58434462" />

    <TextView
        android:id="@+id/async_coroutine_job_hash_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="58434462" />

    <TextView
        android:id="@+id/launch_in_coroutine_job_hash_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="58434462" />

    <Button
        android:id="@+id/create_error_flow_response_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/create_flow_response_btn_text" />

    <Button
        android:id="@+id/start_coroutine_by_launch_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/start_coroutine_by_launch_btn_text" />

    <Button
        android:id="@+id/start_coroutine_by_async_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/start_coroutine_by_async_btn_text" />

    <Button
        android:id="@+id/start_coroutine_by_launch_in_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/start_coroutine_by_launch_in_btn_text" />

</LinearLayout>